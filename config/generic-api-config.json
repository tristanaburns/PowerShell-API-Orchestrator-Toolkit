  },
  "Endpoints": {
    "Posts": {
      "List": "/posts",
      "Get": "/posts/{id}",
      "Create": "/posts",
      "Update": "/posts/{id}",
      "Delete": "/posts/{id}"
    },
    "Users": {
      "List": "/users",
      "Get": "/users/{id}",
      "Create": "/users",
      "Update": "/users/{id}",
      "Delete": "/users/{id}"
    },
    "Comments": {
      "List": "/comments",
      "Get": "/comments/{id}",
      "ByPost": "/posts/{postId}/comments"
    }
  },
